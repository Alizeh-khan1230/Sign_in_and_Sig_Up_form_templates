<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/text"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:backgroundTint="@color/purplebackground"
    android:foregroundTint="@color/white">

    <Button
        android:id="@+id/button3"
        android:layout_width="67dp"
        android:layout_height="71dp"
        android:background="@drawable/buttonround"
        android:text="."
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="225dp"
        tools:layout_editor_absoluteY="730dp" />

    <ImageView

        android:id="@+id/imageView3"
        android:layout_width="239dp"
        android:layout_height="218dp"
        android:layout_marginTop="60dp"
        android:src="@drawable/logo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button5"
        android:layout_width="135dp"
        android:layout_height="31dp"
        android:backgroundTint="@color/purplebackground"
        android:text="Existing"
        android:textColor="@color/purplebackground"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="338dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="343dp"
        android:layout_height="57dp"
        android:layout_marginTop="52dp"
        android:backgroundTint="@color/white"
        android:text="New User"
        android:textColor="@color/purplebackground"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="25dp" />

    <com.google.android.material.chip.Chip
        android:id="@+id/chip"
        android:layout_width="283dp"
        android:layout_height="151dp"
        android:text="."
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="406dp" />

    <EditText
        android:id="@+id/editTextText"
        android:layout_width="265dp"
        android:layout_height="49dp"
        android:drawableRight="@drawable/baseline_person_24"
        android:ems="10"
        android:hint="Username"
        android:text="Alizeh Khan"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="415dp" />

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="272dp"
        android:layout_height="44dp"
        android:drawableRight="@drawable/baseline_lock_24"
        android:ems="10"
        android:hint="Password"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="464dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Log In"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="520dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="154dp"
        android:layout_height="34dp"
        android:backgroundTint="@color/white"
        android:foregroundTint="@color/white"
        android:text="Forgot Password?"
        android:textColor="@color/white"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="138dp"
        tools:layout_editor_absoluteY="592dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="69dp"
        android:layout_height="71dp"
        android:background="@drawable/buttonround"
        android:backgroundTint="@color/white"
        android:text="."
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="730dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="49dp"
        android:layout_height="48dp"
        app:srcCompat="@drawable/google"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="102dp"
        tools:layout_editor_absoluteY="695dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="62dp"
        android:layout_height="44dp"
        app:srcCompat="@drawable/fb"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="225dp"
        tools:layout_editor_absoluteY="695dp" />

    <LinearLayout
        android:layout_width="137dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="681dp"
        tools:ignore="MissingConstraints"></LinearLayout>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="102dp"
        android:layout_height="102dp"
        android:text="OR"
        android:textColor="@color/white"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="190dp"
        tools:layout_editor_absoluteY="618dp" />

</androidx.constraintlayout.widget.ConstraintLayout>